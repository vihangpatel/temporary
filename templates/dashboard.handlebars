<div class="dashboard">

	<div class="active">
		<div class="logo-container">
			<img src="/images/logo.png" />
		</div>

		<div class="input-box">
			<input type="number" />
			<button id="submit-btn"> Enter Token </button>
		</div>
	</div>

	<div class="in-active">

		<div class="logo-container">
			<img src="/images/logo.png" />
		</div>
		<h4> Admin Dashboard </h4>
		<table>

			<thead>
				<tr>

					<td>
						Name
					</td>

					<td>
						Email
					</td>

					<td>
						Contact No.
					</td>

					<td>
						County
					</td>

					<td>
						Previous Treatment
					</td>

					<td>
						Source Of Lead
					</td>

					<td>
						Medical Condition
					</td>

					<td>
						Request Time
					</td>

				</tr>
			</thead>

			<tbody>

				<% _.each((contacts || []).reverse(), function(contact, index) { %>

					<tr>

						<td>
							{{ contact.name }}
						</td>

						<td>
							<a href="mailto:{{ contact.email }}">
								<i class="fa fa-envelope"></i>
							</a>

							{{ contact.email }}
						</td>

						<td>
							<a href="tel:{{ contact.phone }}">
								<i class="fa fa-phone"></i>
							</a>
							{{ contact.phone }}
						</td>

						<td>
							{{ contact.resident }}
						</td>

						<td>
							{{ contact.treatment }}
						</td>

						<td>
							{{ contact.source }}
						</td>

						<td>
							{{ contact.condition }}
						</td>

						<td>
							<% var dateObj = new Date(contact.time * 1000) %>
								{{ dateObj.toDateString() + ' , ' + dateObj.toLocaleTimeString() }}
						</td>

					</tr>

					<% }); %>

			</tbody>

		</table>

	</div>

</div>